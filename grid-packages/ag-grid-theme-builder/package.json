{
  "name": "ag-grid-theme-builder",
  "private": true,
  "version": "31.0.2",
  "scripts": {
    "build": "npm run codegen:prod && tsc --project src && vite build",
    "dev": "npm run codegen:dev && concurrently vite npm:codegen:watch",
    "clean": "rimraf ./dist",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "dev-test": "vitest",
    "build-css": "tsx src/ag-grid-community-themes/scripts/codegen.ts",
    "build-types": "tsx src/ag-grid-community-themes/scripts/codegen.ts",
    "codegen:prod": "tsx src/ag-grid-community-themes/scripts/codegen.ts",
    "codegen:dev": "tsx src/ag-grid-community-themes/scripts/codegen.ts --dev --no-clean",
    "codegen:watch": "chokidar 'src/ag-grid-community-themes/**/*' --ignore '**/GENERATED-*' --command 'npm run codegen:dev'"
  },
  "dependencies": {
    "@ag-grid-community/client-side-row-model": "^31.1.1",
    "@ag-grid-community/core": "^31.1.1",
    "@ag-grid-community/react": "^31.1.1",
    "@ag-grid-enterprise/advanced-filter": "^31.1.1",
    "@ag-grid-enterprise/charts": "^31.1.1",
    "@ag-grid-enterprise/charts-enterprise": "^31.1.1",
    "@ag-grid-enterprise/clipboard": "^31.1.1",
    "@ag-grid-enterprise/column-tool-panel": "^31.1.1",
    "@ag-grid-enterprise/filter-tool-panel": "^31.1.1",
    "@ag-grid-enterprise/menu": "^31.1.1",
    "@ag-grid-enterprise/range-selection": "^31.1.1",
    "@ag-grid-enterprise/rich-select": "^31.1.1",
    "@ag-grid-enterprise/row-grouping": "^31.1.1",
    "@ag-grid-enterprise/set-filter": "^31.1.1",
    "@ag-grid-enterprise/status-bar": "^31.1.1",
    "@carbon/icons-react": "^11.26.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@floating-ui/react": "^0.26.9",
    "ag-charts-enterprise": "^9.1.1",
    "concurrently": "^8.2.2",
    "jotai": "^2.4.2",
    "react": "18.2.0",
    "react-collapse": "^5.1.1",
    "react-collapsed": "^4.1.2",
    "react-dom": "18.2.0",
    "react-shadow": "^20.4.0",
    "react-tabs": "^6.0.2",
    "throttle-debounce": "^5.0.0"
  },
  "devDependencies": {
    "@swc/plugin-emotion": "2.5.86",
    "@types/postcss-import": "^14.0.3",
    "@types/postcss-url": "^10.0.4",
    "@types/react": "^18.2.15",
    "@types/react-collapse": "^5.0.4",
    "@types/react-dom": "^18.2.7",
    "@types/throttle-debounce": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "autoprefixer": "^10.4.19",
    "chokidar-cli": "^3.0.0",
    "cssnano": "^6.1.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "glob": "^10.3.10",
    "postcss-import": "^16.0.1",
    "postcss-nesting": "^12.0.1",
    "postcss-rtlcss": "^4.0.9",
    "postcss-url": "^10.1.3",
    "prettier": "^3.0.3",
    "sass": "^1.71.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^4.4.5",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.4"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie >= 0",
    "not ie_mob >= 0",
    "not blackberry > 0"
  ]
}
